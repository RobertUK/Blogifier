  "": null,
  "AllowedHosts": "*",
  "Blogifier": {
    "DbProvider": "SQLite",
    "ConnString": "Data Source=Data/Blog.db",
    "Salt": "SECRET-CHANGE-ME!",
    "DemoMode": false,
    "FileExtensions": "png,gif,jpeg,jpg,zip,7z,pdf,doc,docx,xls,xlsx,mp3,mp4,avi",
    "PathBase": "/robert/"
  },
  
    "Serilog": {
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "MinimumLevel": "Debug",
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "File",
          "Args": {
            "path": "Logs/log-.txt",
            "rollingInterval": "Day"
          }
        }
      ],
      "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
    
  }

}
